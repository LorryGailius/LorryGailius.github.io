---
import AboutMe from "../components/AboutMe.astro";
import IntroCard from "../components/IntroCard.astro";
import MainLayout from "../layouts/MainLayout.astro";
const title = "About";
---

<MainLayout
  title="Laurynas Gailius | About"
  seoTitle="Laurynas Gailius | About"
  seoDesc="Laurynas Gailius - A beginner Software Engineer looking to start his career in the IT world with deep focus on fullstack programming"
>
  <main
    class="m-auto mt-5 grid gap-2 max-w-2xl relative px-5 sm:px-0 sm:gap-2 md:gap-3 md:grid-cols-2 lg:gap-4"
  >
    <IntroCard />
    <AboutMe />
  </main>
</MainLayout>
